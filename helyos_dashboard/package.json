{
  "name": "helyos-admin-dashboard",
  "version": "2.2.0",
  "description": "helyos Admin Dashboard",
  "license": "Modified Apache-2.0",
  "scripts": {
    "ng": "ng",
    "start": "ng serve",
    "build:dev": "ng build --configuration development",
    "build": "ng build --configuration production --base-href /dashboard/",
    "gitbuild": "ng build --configuration production --base-href **/dist/",
    "test": "ng test",
    "test-ci": "TEST_CI=true ng test",
    "lint": "npm-run-all --continue-on-error lint:eslint lint:stylelint \"lint:prettier:html -- --check\" \"lint:prettier:style -- --check\"",
    "lint:fix": "npm-run-all --continue-on-error \"lint:eslint -- --fix\" \"lint:stylelint -- --fix\" \"lint:prettier:html -- --write\" \"lint:prettier:style -- --write\"",
    "lint:ci": "npm run lint:eslint && npm run lint:stylelint && npm run lint:prettier:html -- --check && npm run lint:prettier:style -- --check",
    "lint:eslint": "ng lint",
    "lint:stylelint": "stylelint 'src/**/*.{css,scss}'",
    "lint:prettier:html": "prettier --parser=angular --log-level=warn src/**/*.html",
    "lint:prettier:style": "prettier --log-level=warn src/**/*.{css,scss}"
  },
  "private": true,
  "dependencies": {
    "@angular/animations": "^17.3.8",
    "@angular/common": "^17.3.8",
    "@angular/compiler": "^17.3.8",
    "@angular/core": "^17.3.8",
    "@angular/forms": "^17.3.8",
    "@angular/localize": "^17.3.8",
    "@angular/platform-browser": "^17.3.8",
    "@angular/platform-browser-dynamic": "^17.3.8",
    "@angular/router": "^17.3.8",
    "@fortawesome/fontawesome-free": "^6.6.0",
    "@ng-bootstrap/ng-bootstrap": "^16.0.0",
    "@ngx-translate/core": "^15.0.0",
    "@ngx-translate/http-loader": "^8.0.0",
    "bootstrap": "^5.3.2",
    "cheap-ruler": "^3.0.2",
    "core-js": "^3.38.0",
    "helyosjs-sdk": "2.2.1",
    "js-yaml": "^4.1.0",
    "lodash": "^4.17.21",
    "rxjs": "^7.4.1",
    "tslib": "^2.6.3",
    "zone.js": "^0.14.10"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "^17.3.8",
    "@angular-eslint/builder": "17.5.2",
    "@angular-eslint/eslint-plugin": "17.5.2",
    "@angular-eslint/eslint-plugin-template": "17.5.2",
    "@angular-eslint/schematics": "17.5.2",
    "@angular-eslint/template-parser": "17.5.2",
    "@angular/cli": "^17.3.8",
    "@angular/compiler-cli": "^17.3.8",
    "@angular/language-service": "^17.3.8",
    "@types/jasmine": "^5.1.4",
    "@types/jasminewd2": "^2.0.13",
    "@types/js-yaml": "^4.0.9",
    "@types/node": "18.17.1",
    "@typescript-eslint/eslint-plugin": "7.11.0",
    "@typescript-eslint/parser": "7.11.0",
    "codelyzer": "^6.0.2",
    "eslint": "^8.57.1",
    "jasmine-core": "^5.2.0",
    "jasmine-spec-reporter": "^7.0.0",
    "karma": "^6.4.4",
    "karma-chrome-launcher": "^3.2.0",
    "karma-cli": "^2.0.0",
    "karma-coverage-istanbul-reporter": "^3.0.3",
    "karma-jasmine": "^5.1.0",
    "karma-jasmine-html-reporter": "^2.1.0",
    "npm-run-all": "^4.1.5",
    "prettier": "^3.3.3",
    "stylelint": "^16.9.0",
    "stylelint-config-standard-scss": "^13.1.0",
    "stylelint-scss": "^6.6.0",
    "ts-node": "^10.9.2",
    "tslint": "^6.1.3",
    "typescript": "^5.4.5"
  }
}
